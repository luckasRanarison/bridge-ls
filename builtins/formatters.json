{
  "prettierd": {
    "command": "prettierd",
    "args": [
      "$filename"
    ],
    "filetypes": [
      "js",
      "ts",
      "jsx",
      "tsx",
      "json",
      "yaml",
      "yml",
      "css",
      "scss",
      "html",
      "md"
    ],
    "toStdin": true,
    "cleanup": "pkill prettierd"
  },
  "rustfmt": {
    "command": "rustfmt",
    "args": [
      "--edition",
      "2021",
      "--emit",
      "stdout"
    ],
    "filetypes": [
      "rs"
    ],
    "toStdin": true
  },
  "stylua": {
    "command": "stylua",
    "args": [
      "-"
    ],
    "filetypes": [
      "lua"
    ],
    "toStdin": true
  }
}
